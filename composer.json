{
    "name": "phpzendo/php-comparison",
    "description": "The most awesome comparison engine created for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "huliuqing",
            "email": "huliuqing1989@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "sebastian/comparator": "^3.0",
        "florianwolters/component-core-comparable": "0.2.*"
    },
    "require-dev": {
        "symfony/var-dumper": "3.4.x-dev",
        "phpunit/phpunit": "^7.2@dev"
    },
    "autoload": {
        "psr-4": {
            "PhpZendo\\Comparison\\": "src/Comparison/"
        },
        "files": [
            "src/Comparison/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "PhpZendo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
